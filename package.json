{
  "name": "onix-codelist",
  "version": "3.0.1",
  "description": "ONIX 3.0 Code lists",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "generate": "rimraf src && ts-node scripts/generate-lists.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --quiet --fix",
    "lint:scripts": "eslint scripts/**/*.ts",
    "lint:scripts:fix": "eslint scripts/**/*.ts --quiet --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/ramda": "^0.28.15",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "camel-case": "^4.1.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "handlebars": "^4.7.7",
    "htmlparser2": "^8.0.1",
    "husky": "^4.3.8",
    "ramda": "^0.28.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "xml-js": "^1.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teunmooij/onix-codelist.git"
  },
  "keywords": [
    "onix",
    "codelist",
    "editeur"
  ],
  "author": "Teun Mooij",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teunmooij/onix-codelist/issues"
  },
  "homepage": "https://github.com/teunmooij/onix-codelist#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:scripts:fix",
      "pre-push": "npm run generate && npm run typecheck"
    }
  }
}
